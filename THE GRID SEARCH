def gridSearch(G, P):
    R, C = len(G), len(G[0])
    r, c = len(P), len(P[0])
    for i in range(R - r + 1):
        for j in range(C - c + 1):
            if G[i][j:j + c] == P[0]:
                found = True
                for k in range(1, r):
                    if G[i + k][j:j + c] != P[k]:
                        found = False
                        break
                if found:
                    return "YES"
    return "NO"
